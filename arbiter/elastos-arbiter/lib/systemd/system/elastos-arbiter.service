[Unit]
Description=Elastos Arbiter Node Daemon
After=network-online.target

[Service]
Type=simple
WorkingDirectory=/data/elastos/arbiter
EnvironmentFile=/etc/elastos-ela/params.env
ExecStart=/usr/local/bin/elastos-arbiter    \
            -p ${KEYSTORE_PASSWORD}

User=elauser
Group=elauser
Restart=always

[Install]
WantedBy=multi-user.target